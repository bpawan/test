          <xs:documentation>Kennzeichen, dass die Rentenzahlungen der Abgeltungsteuer unterliegen</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dauerWartezeitJ" type="d0:CVSlvWartezeit">
