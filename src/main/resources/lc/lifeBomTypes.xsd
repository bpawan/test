                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="waehrungId" type="d0:CVSlvWaehrung">
                        <xs:annotation>
                            <xs:documentation>Das Attribut dient zur eindeutigen Identifizierung einer Waehrung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="verarbDat" type="xs:date">
