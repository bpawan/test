                            <xs:documentation>Erstes Schlüsselfeld für allgemeine Vertragsbedingungen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="avb2Id" type="d0:CVSlvAVB2">
                        <xs:annotation>
                            <xs:documentation>Zweites Schlüsselfeld für allgemeine Vertragsbedingungen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="avb3Id" type="d0:CVSlvAVB3">
                        <xs:annotation>
                            <xs:documentation>Drittes Schlüsselfeld für allgemeine Vertragsbedingungen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="avb4Id" type="d0:CVSlvAVB4">
