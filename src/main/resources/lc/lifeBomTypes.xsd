                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="hvGruppeId" type="xs:short"/>
                    <xs:element minOccurs="1" name="finTyp" type="d0:CVSlvFinTyp">
                        <xs:annotation>
                            <xs:documentation>Finanzierungstyp</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="wterm" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Wertstandsdatum bis zu dem die Geldflüsse bzw. zu dem das Vermögen ermittelt wurde</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
