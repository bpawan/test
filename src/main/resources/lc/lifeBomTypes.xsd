    <xs:complexType name="HvGruppe">
        <xs:annotation>
            <xs:documentation>Im Rahmen der bAV können unterschiedliche Zahlweisen für Arbeitgeber (AG) und Arbeitnehmer (AN)
                vorgegeben werden. Die notwendigen Informationen werden pro HV bzw. in einer hvGruppe geführt,
                die die eigentliche HV und die zugehörigen ZV zusammenfasst. Die hvGruppe entspricht bei
                Pensionsfondsprodukten der Vertragsschicht für den AG bzw. dem AN. Die Entität hvGruppe enthält
                die Zahlweise und alle sonstigen Größen, die für die HV und die zugeordneten ZV geführt werden
                müssen.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="btgAuftBerech">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="btgAuftBerech" type="lifeBom:BtgAuftBerech"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="btgAuftErg">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="btgAuftErg" type="lifeBom:BtgAuftErg"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="btgAuftReg">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="btgAuftReg" type="lifeBom:BtgAuftReg"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="btgWerte">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="btgWerte" type="lifeBom:BtgWerte"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="kinderzulage">
