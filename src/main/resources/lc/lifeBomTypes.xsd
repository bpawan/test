                    <xs:element minOccurs="0" name="notizText" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Inhalt einer Notiz.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="bearbeiterId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Identifikation des Bearbeiters einer Aktion (z.B. eines Geschaeftsvorfalls).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="datleae" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Datum der letzten aenderung des Datensatzes.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kz_courtage" type="xs:int">
