                            <xs:documentation>bei Beitrageingang: Erhöhung der endfälligen Garantie bei Fullreset: Neuer Gesamtbetrag der endfälligen Garantie</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="wirkBeginn" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>SNTAuftverh.wirkbeginn, Wirksamkeitstermin des zu verarbeitenden GeVos.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Termin, an dem das Objekt bearbeitet wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="neuInvest" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>bei Beitragseingang: Sparbeitrag bei Fullreset:  neues Vermögen</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garantBudget" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>nominelle Garantiegebühr</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="prozKosten1" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Kostensatz 1 zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="prozKosten2" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Kostensatz 2 zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="prozKosten3" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Kostensatz 3 zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="guebisKosten1" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Gültigkeitsende des 1. Kostensatzes zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="guebisKosten2" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Kostensatz 2 zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="guebisKosten3" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Gültigkeitsende des 3. Kostensatzes zur Optionspreisbewertung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garTodFF" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>garantierte TFL aus Sparbeiträgen zum freien Fonds</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garTodWS" type="xs:double">
