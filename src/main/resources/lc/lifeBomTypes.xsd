                    <xs:element minOccurs="0" name="hvGruppeId" type="xs:short"/>
                    <xs:element minOccurs="0" name="vtId" type="xs:short"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="JurVT">
        <xs:annotation>
            <xs:documentation>Die Rows in Table jurVT repräsentieren jeweils genau einen Vertragsteil.</xs:documentation>
        </xs:annotation>
