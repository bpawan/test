    <xs:complexType name="Auftverh">
        <xs:annotation>
            <xs:documentation>In der Tabelle Aufteilungsverhältnis wird für jeden relevanten Geschäftsvorfall die Aufteilung des Beitrags / der Zuzahlung oder bei einem FullReset des Gesamtfondsvermögens der HV vorgegeben. Diese Tabelle wird alternativ zum Verteilerschlüssels Vertschl benutzt, da die Aufteilung sich jedes Mal ändern kann.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="kzNeuesAuftverh" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen, ob ein neues Aufteilungsverhältnis berechnet wurde</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzFullReset" type="xs:short">
