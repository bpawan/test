                            <xs:documentation>Auszahlungsbetrag bei Leistung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="btgSteu" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Gesamtbeitrag des Vertragsteils im steuerlichen Sinne (bei Beitragstyp gefördert: enthält auch Zulagen)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="vermoegen" type="xs:double">
