                    <xs:element minOccurs="0" name="dokNwVar">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="dokNwVar" type="lifeBom:DokNwVar"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokTypId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Schlüsselsystem für den Dokumenttyp</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbIdSto" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Wird eine Bearbeitung storniert, enthält die bearbIdSto des aktuellen Vorgangs die bearbId des stornierenden Vorgangs.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Termin, an dem das Objekt bearbeitet wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbDatSto" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Bearbeitungstermin des stornierenden Vorgangs</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbeiterId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Identifikation des Bearbeiters einer Aktion (z.B. eines Geschäftsvorfalls).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="wirkBeginn" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Termin, ab dem der neue Vertragsstand fachlich wirksam ist.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="wirkEnde" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Termin, ab dem der Vertragsstand fachlich nicht mehr wirksam ist.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="druckOrt" type="d0:CVSlvDruckOrt">
                        <xs:annotation>
                            <xs:documentation>Schlüsselsystem: Zentral, am Arbeitsplatz</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="anzahlDruck" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Anzahl der zu druckenden Kopien</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzVersand" type="d0:CVSlvVersandKz">
                        <xs:annotation>
                            <xs:documentation>Versandart (Versand-Kz) wie Direktversand, Aussteuern - SB, über Filialdirektion, über Außendienstpartner, ...</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="schrittId" type="d0:CVSlvSchrittID">
                        <xs:annotation>
                            <xs:documentation>Eindeutige Identifizierung eines Arbeitsschritts</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzAktBearb" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Steuert, ob ein Datensatz (z.B. ein VT) vom SB in der aktuellen Bearbeitung angelegt wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzVersendet" type="d0:CVSlvKzVersendet">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen, ob ein Dokument versendet (bzw. Richtung Drucksystem weitergegeben) wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="pendingModificationIdExt" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Externe Id einer pending modification (Schwebe in LJFEE).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokNwId" type="xs:int"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DokNwExt">
        <xs:annotation>
            <xs:documentation>dokNwExt</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="anrId" type="d0:CVSlvBriefAnrede">
                        <xs:annotation>
                            <xs:documentation>Die Anrede kann z.B. für ein Anschreiben oder die Adresse verwendet werden.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="titelId" type="d0:CVSlvTitel">
                        <xs:annotation>
                            <xs:documentation>Hier werden die möglichen Titel eines Partners verschlüsselt. Sie können einer natürlichen Person zugeordnet werden.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="name" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Nachname einer Person.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="vorname" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Vorname des Partners, falls er eine natürliche Person ist.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="strasse" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Bezeichnung für eine Straße, einen Platz, einen Weg etc. incl. der Hausnummer.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="plz" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Postleitzahl der Straßenadresse</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="wohnort" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Wohnort der Person</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="ortsZusatz" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Ortszusatz, z.B. Bad Homburg v.d.H. oder Stegen am Ammersee</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="orgEinheit" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Organisationseinheit, der der Vermittler zugeordnet ist (z.B. Regionaldirektion)Organisationseinheit ist auch Abteilungsbezeichner, wenn der Partner gleichzeitig Sachbearbeiter beim VU ist</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="nachricht" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Freier Text für ein Nachrichten-Kennzeichen</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="betreff" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Freier Text für einen Betreff</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokKopfId" type="d0:CVSlvKopfzeile">
                        <xs:annotation>
                            <xs:documentation>Schlüsselverzeichnis, das den Dokumentkopf identifiziert</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokFussId" type="d0:CVSlvFusszeile">
                        <xs:annotation>
                            <xs:documentation>Schlüsselverzeichnis, das den Dokumentfuß identifiziert</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="abwErstDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Das Datum, an dem das Dokument erstellt wurde; dies kann nach Disposition des Sachbearbeiters verschieden vom Tagesdatum sein</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="land" type="d0:CVLand">
                        <xs:annotation>
                            <xs:documentation>Land (String, len=6)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="adrZus1" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Adreßzusatz Zeile 1</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="ihrZeichen" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Referenz zur Identifikation des Empfängers</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="abwBearbeiterId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Falls abwBearbeiterId IS NOT NULL, so sind der aktuelle Bearbeiter des gesendeten Briefs (in Column dokNw.bearbeiterId) und der Ansprechpartner für diesen Brief verschieden. In diesem Fall enthält dokNwExt.abwBearbeiterId den Namen des Ansprechpartners.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="partnerId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Global eindeutige Identifikation eines Partners; größer als Null</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="postfach" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Hier wird das Postfach innerhalb einer Adreßangabe geführt, falls keine Straße bekannt ist.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="poPlz" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Postleitzahl der Postfachadresse</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="adrZus2" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Adreßzusatz Zeile 2</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="adelId" type="d0:CVSlvAdel">
                        <xs:annotation>
                            <xs:documentation>Schlüsselverzeichnis, das den Adelstitel identifiziert.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bundesland" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Bundesland, Bundesstaat, Kanton, Provinz, etc...</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="partnerIdExt" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>externe Darstellung der Identifikation eines Partners</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="spracheDok" type="d0:CVSlvSprache">
                        <xs:annotation>
                            <xs:documentation>Dokumentensprache</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokNwId" type="xs:int"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="DokNwVar">
        <xs:annotation>
            <xs:documentation>Die Table dokNwVar dokumentiert die Werte der bei einem konkreten Dokument verwendeten aktuellen Dokumentvariablen.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="dokVarValue" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Inhalt der Dokumentvariable; alle Datentypen werden hier in ihrer String-Form dargestellt</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokVarNameLang" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Sichtbare Bezeichnung einer Dokumentvariable im Dialog</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="dokNwId" type="xs:int"/>
                    <xs:element minOccurs="1" name="dokVarName" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Identifikation einer Dokumentvariable</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SchwebeNw">
        <xs:annotation>
            <xs:documentation>Eine Schwebe ist eine Aenderungsanweisung für einen Vertrag derart, dass die Aenderung weder ausgeführt noch wirksam wird, sondern in einer Art Postfach bis zu einem bestimmten Ereignis ruht.Im Postfach eines Vertrages können mehrere Schweben gleichzeitig liegen. Als Ereignisse, die die tatsächliche Vertragsänderung anstossen, kommen in Frage:* das Vorhandensein aller notwendigen Informationen* die erfolgreiche Prüfung durch zweite und dritte Sachbearbeiter* für weit in der Zukunft liegenden Aenderungen ist der richtige Termin eingetretenEine Schwebe hat einen Zustand, der sich im Laufe der Bearbeitung ändert.Im Schwebenachweis werden unterbrochene Verarbeitungen abgelegt. Hierzu wird der Verarbeitungszustand, der während der Verarbeitung im ABFProxy gehalten wird, im Schwebenachweis persistiert. Dies geschieht durch Ablage des aktuellen Verarbeitungszustandes im XML-Format im Attribut proxyKontext.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="schwebeNwExt" type="lifeBom:SchwebeNwExt"/>
                    <xs:element minOccurs="0" name="wirkDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Das Field wirkDat enthält das Datum, an dem die Vertragsänderung, durch die diese Schwebe angelegt wurde, wirksam werden sollte.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="meldedat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Das Field meldeDat enthält das Datum, an dem der Grund der Schwebe dem VU gemeldet wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Termin, an dem das Objekt bearbeitet wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbeiterId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Identifikation des Bearbeiters einer Aktion (z.B. eines Geschäftsvorfalls).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="schwebeTyp" type="d0:CVSlvSchwTyp">
                        <xs:annotation>
                            <xs:documentation>Typ der Schwebe.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="schwebeStatus" type="d0:CVSlvSchwStatus">
                        <xs:annotation>
                            <xs:documentation>Status der Schwebe.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="gevoId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Die gevoId gibt den im EBF-Server durchgeführten Geschäftsvorfall an.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="schrittId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Das Field schrittId enthält die Schritt-Id, das die Vertragsänderung, durch die diese Schwebe angelegt wurde, hatte.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbGrdId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Schlüsselverzeichnis für den Grund der Bearbeitung (z.B. Beitragsreduktion aufgrund eines Kundenwunsches, aufgrund Mahnung)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="todDat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Datum, an dem eine natürliche Person gestorben ist.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="schwebeId_4augen" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>schwebeId der Schwebe, die für die Vier-Augen-Freigabe gespeichert wurde</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="wertbez" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Datum, das in die Berechnung des Wertstellungstermins als Bezugsgroesse eingeht.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bewertResTerm" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>abweichender Bewertungsreservetermin</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="schwebeId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Eindeutige Identifikation eines Schwebenachweises innerhalb eines Vertrags. Teil des Primärschlüssels.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SchwebeNwExt">
        <xs:annotation>
            <xs:documentation>schwebeNwExt enthält Teile des ProxyKontext.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="proxyKontext" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Ablage des Proxy-Verarbeitungskontext als XML-String.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="aktiv" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Für aktive Schweben wird dieses Attribut mit JA, für Backup Schweben mit NEIN gesetzt.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="schwebeId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Eindeutige Identifikation eines Schwebenachweises innerhalb eines Vertrags. Teil des Primärschlüssels.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="fragmentId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Nummer des Fragments aus dem ProxyKontext.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="schwebeVersion" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Wird verwendet um festzustellen, ob die Schwebe migriert werden muß.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ProvDaten">
        <xs:annotation>
            <xs:documentation>Führung historisch relevanter Provisionsdaten am Vertrag</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="abAgenturId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Nummer der Abschluß-Agentur</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="beAgenturId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Nummer der Bestands-Agentur</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="miAgenturId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Nummer der mitwirkenden Agentur</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzManProv" type="d0:CVSlvJaNein">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen, ob die Provision manuell zu vergeben oder zu verändern ist</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzVertrAktion" type="d0:CVSlvKzVertrAktio">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen für Vertriebsaktionen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzWettbew" type="d0:CVSlvKzWettbew">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen für Vertriebswettbewerbe</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bewsfakt" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Bewertungsfaktor bei der Provisionierung</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="provKuerz" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Faktor zur Kürzung der Provision</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzProvDyn" type="d0:CVSlvKzProvDyn">
                        <xs:annotation>
                            <xs:documentation>Gibt an, wie im Falle eines Vermittler-Wechsels die Provision aus Dynamik aufgeteilt wird.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzStores" type="d0:CVSlvKzStores">
                        <xs:annotation>
                            <xs:documentation>Gibt Besonderheiten bei der Behandlung der Stornoreserve an.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzMarket" type="d0:CVSlvKzMarket">
                        <xs:annotation>
                            <xs:documentation>Enthält eine Kennzeichnung des Marketings.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="vertriebsweg" type="d0:CVSlvVertriebsweg">
                        <xs:annotation>
                            <xs:documentation>Vertriebsweg</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="maxZinsMod" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>maximaler Zinssatz für die Modellrechnung, vertriebswegabhängig</xs:documentation>
                        </xs:annotation>
                    </xs:element>
