                        <xs:annotation>
                            <xs:documentation>Kennzeichen, ob die Risikoklasse vom Sachbearbeiter direkt erfasst wurde</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzAktBearb" type="d0:CVSlvJaNein">
                        <xs:annotation>
                            <xs:documentation>Steuert, ob ein Datensatz (z.B. ein VT) vom SB in der aktuellen Bearbeitung angelegt wurde</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzInitialKBF" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Steuert, ob ein Datensatz (z.B. ein VT) mit Daten aus Muster vorbelegt wurde.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="kzZuschlagTarifRel" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Kennzeichen, ob die Risikozuschläge bei der Tarifierung zu berücksichtigen sind (1) oder nicht (0).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="vtId" type="xs:short"/>
                    <xs:element minOccurs="0" name="partnerId" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Das Field partnerId identifiziert die versicherte Person, zu der diese Row eine Aussage macht.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="1" name="rollenId" type="d0:CVSlvPartnerrolle">
                        <xs:annotation>
                            <xs:documentation>Identifikation einer Partnerrolle in einer Partner-Vertrags-BeziehungConstraint rollenId_RANGE: nur die Rolle 'versicherte Person' darf vorkommen.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="VtTa">
