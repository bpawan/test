                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garantWS" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>endfällige Garantie zum WSF </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garantFF" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>endfällige Garantie zum FFAnteil des freien Fonds an der Garantie</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="garantErh" type="xs:double">
