    <xs:complexType name="ContractHistoryInfo">
        <xs:annotation>
            <xs:documentation>The ContractHistoryInfo structure contains a minimal set of administrative information about the given
                contract modification. This is used as a list item in the contract modification history.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="util:BusinessObjectBase">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="isModificationUnscheduled" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>True if the modification is an unscheduled one (e.g. based on customers request).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="modifiedAt" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Modification date (when the modification was executed).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="modifiedBy" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Name / Identifier of the user executing the modification (who executed the modification - signature right).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="modifiedBySignatureLeft" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Name / Identifier of the user approving the modification (who approved the modification - signature left).
                                The attribute is set only in case if the system enforced this additional approval (second-set-of-eyes principle).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="modificationReasonCV" type="d0:CVReasonOfModificationsType">
                        <xs:annotation>
                            <xs:documentation>The reason of the modification	(why is the contract modified).
                                In case of informational services or when loading a contract the value can be set to UNDEFINED.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="versionNo" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The version number of the given contract state.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="versionNoCancel" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>The version number of the modification which cancelled the given contract version. The attribute is only set,
                                if the given contract version is cancelled / not valid.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="businessProcessTypeCV" type="d0:CVBusinessProcessType">
                        <xs:annotation>
                            <xs:documentation>The type of the executed modification (what ist modified on the contract).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="effectiveDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>The effective date of the modification (wirkBeginn).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="comment" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Comment is a input the user may provide in order to be able to quickly identify one contract history info.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="pendingModificationIdExt" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>In case of a pending modification this field holds the external identifier. In case the external identifier is not set by the client, the system creates one.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="pendingModificationTypeCV" type="d0:CVSlvSchwTyp">
                        <xs:annotation>
                            <xs:documentation>In case of a pending modification this field is filled and identifies the type (e.g. prepared modification, suspended work).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="lf4CompatibilityIsSchwebeNw" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>This attribute is set for compatibility reasons to LifeFactory Version 4 (LF4). It is set to TRUE in case of a pending modification (SchwebeNw) saved from within LF4.
                                Otherwise it is set to FALSE.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="lifeGroupTaskId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>The id of the life group task created this history entry.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="todesursache" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Die Ursache des Todes.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="meldedat" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>Datum der Meldung.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kzDokUnterdr" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>Dokumentiert, ob bei dem GeVo die Standarddokumente vom Sachbearbeiter unterdrückt wurden.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
