                    </xs:element>
                    <xs:element minOccurs="0" name="retroSatz" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Retrozessionssatz in Prozent des Anlagebetrags.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="umsStempel" type="xs:double">
