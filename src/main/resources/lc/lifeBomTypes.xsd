                    <xs:element minOccurs="0" name="beschKE" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Gesamter für die Auszahlung bescheinigter Kapitalertrag (unter Berücksichtigung von Steuerermäßigung und Steuerbefreiungen)</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="antKestErtrag2005" type="xs:double">
