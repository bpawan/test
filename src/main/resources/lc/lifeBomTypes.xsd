    <xs:complexType name="ZahlEmpf">
        <xs:annotation>
            <xs:documentation>Die Zahlungsempfänger können, müssen jedoch nicht als Partner geführt werden.Zahlungsempfaenger: Die Zahlungsempfänger können, müssen jedoch nicht als Partner geführt werden.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="persistence:PersistentObject">
                <xs:sequence>
                    <!-- no xmlId cause the type has an xmlId attribute:  -->
                    <xs:element minOccurs="0" name="antBetrag" type="xs:double">
                        <xs:annotation>
                            <xs:documentation>Absolutbetrag, den der entsprechende Zahlungsempfänger erhält.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="partnerId" type="xs:int">
                        <xs:annotation>
