                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="anlschw" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Anlageschwerpunkt eines Fonds.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="aussch_p" type="d0:CVSlvJaNein">
