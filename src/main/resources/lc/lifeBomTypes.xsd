                            <xs:documentation>Art der Riesterrente, d.h. lebenslange Rente oder Auszahlplan</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="0" name="lvId" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>Das Field lvId identifiziert denVertrag, zu dem diese Row gehört.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="vtId" type="xs:short"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
