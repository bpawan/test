  </xs:complexType>
  <xs:complexType name="BeguenstProposal">
    <xs:annotation>
      <xs:documentation>Die Rows in table beguenst repräsentieren begünstigte Personen eines Vertrages. Es wird festgelegt, wer für die jeweilige Leistungsart der Begünstigte ist.Schlüsselwerte legen fest, wer für die jeweilige Leistungsart (Tod, Erleben, ...) der Begünstigte sein soll (z.B. Eltern der versicherten Person, Ehegatte der versicherten Person, Versicherungsnehmer, etc.). Andere Schlüsselwerte unterschieden zwischen widerruflich und unwiderruflich.Falls die genaue Ausprägung der Begünstigung nicht verschlüsselbar ist, so kann in einem Textfeld die Begünstigung bzw. der Begünstigte bei Bedarf näher beschrieben werden.</xs:documentation>
    </xs:annotation>
    <!-- all properties: [bezRechtId, bezArtId, widerrufId, inhalt, sex, rangBezugsrecht, partnerIdExt, anteil] -->
    <xs:sequence>
      <!-- xmlId -->
      <!-- no xmlId required -->
      <!-- localKeyEntity -->
      <!-- no localKeyEntity available -->
      <xs:element minOccurs="0" name="bezRechtId" type="d0:CVSlvBezugsrecht">
        <xs:annotation>
          <xs:documentation>Kennzeichen zur Beschreibung des Bezugsrechtstyps (Standard, ...).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="bezArtId" type="d0:CVSlvBezrechtart">
        <xs:annotation>
          <xs:documentation>Kennzeichen für die Zuordnung des Bezugsrechts zu Risikoarten (Erleben, Tod, ...).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="widerrufId" type="d0:CVSlvWiderrufKz">
        <xs:annotation>
          <xs:documentation>Kennzeichen für die Qualität eines Bezugsrechts (widerruflich, unwiderruflich, ...)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="inhalt" type="xs:string">
        <xs:annotation>
          <xs:documentation>Dieses Attribut beinhaltet einen Freitext variabler Länge.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="sex" type="d0:CVSlvGeschlecht">
        <xs:annotation>
          <xs:documentation>Identifizierender Schlüssel für das Geschlecht einer natürlichen Person.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="rangBezugsrecht" type="xs:int">
        <xs:annotation>
          <xs:documentation>Rangfolge des personenbezogenen Bezugsrechts; eindeutig innerhalb eines Vertrages.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="partnerIdExt" type="xs:string">
        <xs:annotation>
          <xs:documentation>externe Darstellung der Identifikation eines Partners</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="anteil" type="xs:double">
        <xs:annotation>
          <xs:documentation>Anteil in % des jeweiligen Bezugsrechts.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!-- id properties -->
      <xs:element minOccurs="0" name="beguenstId" type="xs:short"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DrittrechtProposal">
    <xs:annotation>
      <xs:documentation>Die Rows in Table drittrecht repräsentieren berechtigte Interessen Drittberechtigter (z.B. Vormund, Pfandgläubiger, ...) an einer Leistung aus dem Vertrag. Die Wirksamkeit eines Drittrechts ist abhängig von der Anzeige gegenüber dem VU. Während der Dauer z.B. einer Abtretung gilt die festgelegte Begünstigung nachrangig. Unwiderruflich Begünstigte müssen z.B. einer Abtretung zustimmen.</xs:documentation>
    </xs:annotation>
    <!-- all properties: [unanfechtbarkeit, drittrechtTypId, beginn, drittrechtBez, drittrechtErl, drittrechtTod, rang, drittrechtArtId, kzStSchaedZession, abwHoehe] -->
    <xs:sequence>
      <!-- xmlId -->
      <!-- no xmlId required -->
      <!-- localKeyEntity -->
      <!-- no localKeyEntity available -->
      <xs:element minOccurs="0" name="unanfechtbarkeit" type="proposalFilterDef:UnanfechtbarkeitProposal">
      </xs:element>
      <xs:element minOccurs="0" name="drittrechtTypId" type="d0:CVSlvDrittrecht">
        <xs:annotation>
          <xs:documentation>Identifizierung eines zulässiges Drittrechts</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="beginn" type="xs:date">
        <xs:annotation>
          <xs:documentation>Termin, ab dem die Vereinbarung wirksam wird.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="drittrechtBez" type="xs:string">
        <xs:annotation>
          <xs:documentation>Referenz, auf die sich das Drittrecht bezieht, z.B. Aktenzeichen, Darlehensnummer oder Grundbuchnummer bei einer Hypothekenvergabe o.ä.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="drittrechtErl" type="xs:double">
        <xs:annotation>
          <xs:documentation>Betrag der Drittberechtigung beim Erleben.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="drittrechtTod" type="xs:double">
        <xs:annotation>
          <xs:documentation>Betrag der Drittberechtigung im Todesfall.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="rang" type="xs:int">
        <xs:annotation>
          <xs:documentation>Rangfolge des Drittrechtes, eindeutig innerhalb eines Vertrages.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="drittrechtArtId" type="d0:CVSlvDrittrecht">
        <xs:annotation>
          <xs:documentation>Art des Drittrechts</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="kzStSchaedZession" type="d0:CVSlvJaNein">
        <xs:annotation>
          <xs:documentation>Kennzeichen, ob eine steuerschädliche Zession vorliegt</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="abwHoehe" type="xs:double">
        <xs:annotation>
          <xs:documentation>Abweichende Höhe der Drittrechtsumme</xs:documentation>
        </xs:annotation>
      </xs:element>
      <!-- id properties -->
      <xs:element minOccurs="0" name="drittrechtId" type="xs:int"/>
    </xs:sequence>
  </xs:complexType>
