          <xs:documentation>Höhe des Beitrags in EUR, den der Arbeitgeber finanziert</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="btgAnteilAGProz" type="xs:double">
        <xs:annotation>
          <xs:documentation>Prozentualer Beitragsanteil, den der Arbeitgeber finanziert</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="btgAnteilAN" type="xs:double">
        <xs:annotation>
          <xs:documentation>Höhe des Beitrags in EUR, den der Arbeitnehmer finanziert</xs:documentation>
        </xs:annotation>
      </xs:element>
