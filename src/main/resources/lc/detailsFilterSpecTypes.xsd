            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="feinDat" type="xs:short">
            <xs:annotation>
              <xs:documentation>Feinsteuerungsinformation zum Termin Termin-Wirksamkeit</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="1" name="bearbDat" type="xs:date">
