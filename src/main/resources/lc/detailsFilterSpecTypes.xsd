    <!-- all properties: [bearbDatSto, bilaa, bilnrmres, biltarres, bilbub, bildr, bonres, statAnz, statAnzBfr, statBeitr, statVsum, statVsumBfr, riskap1, riskap2, gebsgrb, btgrest, wirkDat, feinDat, bearbDat, varianteId, ausglHErlKl, ausglHZinslKl, ausglHZinsva, bilsgrRedRefi, gebsgrbRedRefi, aufstockBG, bearbId] -->
