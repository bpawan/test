            <xs:annotation>
              <xs:documentation>Statistische Anzahl beitragsfreier Verträge (z.B. zum Bilanztermin)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="statBeitr" type="xs:double">
            <xs:annotation>
