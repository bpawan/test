                    <!-- id properties -->
                    <xs:element minOccurs="1" name="fondsId" type="xs:int"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SnthvGruppeManualModification">
