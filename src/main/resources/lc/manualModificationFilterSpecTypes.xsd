                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="bearbId" type="xs:int">
                    </xs:element>
                    <!-- id properties -->
                    <xs:element minOccurs="1" name="schichtId" type="xs:short"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
