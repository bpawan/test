            <xs:extension base="util:ChangeVector_3_2_0">
                <xs:sequence>
                    <!-- xmlId -->
                    <!-- no xmlId required -->
                    <!-- localKeyEntity -->
                    <!-- no localKeyEntity available -->
                    <xs:element minOccurs="0" name="drittrecht">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element maxOccurs="unbounded" minOccurs="0" name="drittrecht" type="manualModificationFilterSpec:DrittrechtManualModification">
